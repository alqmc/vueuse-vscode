{
  "name": "VueUse",
  "version": "0.0.1",
  "description": "VueUse tooling for VS Code",
  "keywords": [
    "vscode",
    "extension"
  ],
  "license": "MIT",
  "author": "alqmc",
  "main": "extension.js",
  "publisher": "alqmc",
  "activationEvents": [
    "onLanguage:vue",
    "onLanguage:javascript",
    "onLanguage:teptscript",
    "onView:docssearch",
    "onView:docslibs"
  ],
  "categories": [
    "Snippets",
    "Other"
  ],
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "icon": "public/logo.png",
  "contributes": {
    "languages": [
      {
        "id": "vue",
        "extensions": [
          ".vue"
        ]
      }
    ],
    "configuration": {},
    "snippets": [],
    "commands": [
      {
        "command": "VueUse.previewMd",
        "title": "预览md文档"
      },
      {
        "command": "VueUse.search",
        "title": "检索方法"
      },
      {
        "command": "VueUse.refreshEntry",
        "title": "刷新",
        "icon": {
          "light": "public/refresh-light.svg",
          "dark": "public/refresh-dark.svg"
        }
      }
    ],
    "menus": {
      "view/item/context": [],
      "view/title": [
        {
          "command": "VueUse.refreshEntry",
          "when": "view == docslibs",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "docshome",
          "title": "VueUse",
          "icon": "public/logo.svg"
        }
      ]
    },
    "views": {
      "docshome": [
        {
          "id": "docslibs",
          "type": "webview",
          "name": "函数库",
          "icon": "public/logo.svg"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "docslibs",
        "contents": "暂无数据"
      }
    ]
  }
}
