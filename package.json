{
  "name": "VueUse",
  "description": "VueUse tooling for VS Code",
  "author": "alqmc",
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "build": "gulp --require sucrase/register/ts --gulpfile build/gulpfile.ts vscode",
    "watch": "gulp --require sucrase/register/ts --gulpfile build/gulpfile.ts watchVscode",
    "pkg": "cd dist && vsce package",
    "push": "git push gitee master && git push github master",
    "update:version": "sucrase-node build/version.ts",
    "clear": "rimraf dist",
    "prepare": "husky install",
    "log": "changeloger",
    "lint:js": "eslint -c ./.eslintrc.js --fix --ext .jsx,.js,.vue \"src/**/*.{jsx,js,vue}\" "
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.9",
    "node-fetch": "^2.6.7",
    "vue": "^3.2.39"
  },
  "devDependencies": {
    "@alqmc/build-ts": "^0.0.9",
    "@alqmc/build-utils": "^0.0.5",
    "@alqmc/build-vue": "^0.0.7",
    "@alqmc/eslint-config": "^0.0.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/config-conventional": "^9.1.2",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/gulp": "^4.0.9",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-less": "^0.0.32",
    "@types/node-fetch": "^2.6.2",
    "@types/vscode": "^1.67.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "changeloger": "0.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "husky": "^8.0.1",
    "kolorist": "^1.5.1",
    "lint-staged": "^10.5.4",
    "postcss": "^8.4.16",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sucrase": "^3.20.3",
    "typescript": "^4.6.3",
    "vite": "^3.1.0"
  }
}